pub state<int> count := state(0)

pub fn Counter() {
    float<32> sum := 2 + 3

    int<8> sum := 256;

    return (
        <div>
            <p>The sum of 2 + 3 + 4 is {sum}</p>
            <p>Count: {count}</p>
            <button onclick={fn() { count++ }}>Increment</button>
        </div>
    )
}